SQL> create table schedule
  2         (
  3          day varchar(5),
  4          sess int,
  5          faculty varchar(10),
  6          sem varchar(5),
  7          room varchar(5),
  8          subject varchar(10),
  9          constraints  mycomp1 unique(day,sess,faculty,sem,room),
 10         constraints  mycomp2 unique(day,sess,faculty),
 11         constraints  mycomp3 unique(day,sess,sem),
 12         constraints mycomp4 unique(day,sess,room)
 13         );
 Table created.
 
SQL> insert into schedule values('mon',1,'john','7th','r1','java');

1 row created.

